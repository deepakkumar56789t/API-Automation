          <stringProp name="script">import org.apache.jmeter.threads.JMeterVariables; 
	
try {
	//Clear all user defined variables
	JMeterVariables clean = new JMeterVariables(); 
	vars = clean;

	//Clear all user defined properties
	for (var idx=1; idx &lt;= ${threadCount}; idx++) {
		vars.put(&quot;idx&quot;, idx+&quot;&quot;);
		props.remove(&quot;end_date_&quot; + vars.get(&quot;idx&quot;));
		props.remove(&quot;stop_payment_id_&quot; + vars.get(&quot;idx&quot;));
	}
	
	props.remove(&quot;idx&quot;);
	props.remove(&quot;env&quot;);
	props.remove(&quot;access_token&quot;);
	props.remove(&quot;encryptedData_merchantInfo&quot;);
	props.remove(&quot;encryptedData_create_stoppayment&quot;);
	props.remove(&quot;encryptedData_search_stoppayment&quot;);
} catch (Exception e) {
	log.error(e.getMessage());
}
</stringProp>
